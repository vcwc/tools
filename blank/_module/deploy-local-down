#!/usr/bin/env bash

prefix="vcwc_tools_blank__$1"
prefix="vcwc_tools_blank__$1"

executionplane --silent docker container prune --force

if [[ "vcwc_tools_blank__blank_blank_volume" == "$prefix"* ]] ; then
	continue="true"
	while [[ "true" == "$continue" ]] ;
	do
		executionplane docker volume rm "vcwc_tools_blank__blank_blank_volume"
		result=$(docker volume ls | grep "vcwc_tools_blank__blank_blank_volume")
		if [[ "$result" == "" ]] ; then
			continue="false"
		else
			sleep 3;
		fi
	done;
fi
